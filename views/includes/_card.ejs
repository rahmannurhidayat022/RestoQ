<div class="row my-2">
  <% resto.forEach(e => { %>
    <div class="col-sm-12 col-md-4 col-lg-3">
      <div class="card my-2 mx-1">
        <div class="card-body">
          <h5 class="card-title"><%= e.nama %> </h5>
          <div class="menu row d-block">
            <ul>
              <li>Alamat : <%= e.alamat %> </li>
              <li>Menu Favorit : <%= e.favoriteMenu %> </li>
            </ul>
          </div>
          <p class="card-text"><%= e.deskripsi %> </p>
          <div class="row">
            <div class="col-lg-6 col-md-12 my-1 col-sm-12">
              <a href="javascript:void()" class="btn btn-block btn-warning"
                data-toggle="modal"
                data-target="#edit-modal"
                data-id="<%= e.id %>"
                data-nama="<%= e.nama %>"
                data-alamat="<%= e.alamat %>"
                data-deskripsi="<%= e.dekripsi %>"
                data-favoriteMenu="<%= e.favoriteMenu %>"
              >
                <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
              </a>
            </div>
            <div class="col-lg-6 col-md-12 my-1 col-sm-12">
              <a href="#" class="btn btn-block btn-danger">
                <i class="fa fa-trash" aria-hidden="true"></i>
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
  <% }) %>  
</div>